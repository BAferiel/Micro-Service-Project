FROM openjdk:20
MAINTAINER microserviceproject
ARG JAR_FILE=target*/.jar
EXPOSE 8084
ADD ./../Reviews/target/Reviews-1.0.0.jar app.jar
ENTRYPOINT ["java", "-jar", "app.jar"]
ENV SPRING_DATASOURCE_URL=jdbc:h2:mem:microservice
ENV EUREKA_CLIENT_SERVICEURL_DEFAULTZONE=http://serviceregistry:8761/eureka/